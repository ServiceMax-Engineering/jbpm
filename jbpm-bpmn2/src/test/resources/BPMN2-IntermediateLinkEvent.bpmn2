  <process id='_linkEventProcessExample'>
    
    
    <startEvent id='_A176EE94-8688-46D3-8C3F-B7C01CA73782' name='start jump'/>
    
    <sequenceFlow id='_EE84FA5C-6CF8-4FA0-AD6C-2F40E8D07824' sourceRef='_A176EE94-8688-46D3-8C3F-B7C01CA73782' targetRef="_scriptTask-about-to-jump-id"/>
    
    
    <scriptTask id="_scriptTask-about-to-jump-id" name="task before link">
      <script>System.out.println("about to jump");</script>
    </scriptTask>
    
     <sequenceFlow id='_481A2782-FC17-41CB-BC28-5799B6866A80' sourceRef="_scriptTask-about-to-jump-id" targetRef='_id_1'/>
     
     
     <!-- throw link event -->
     <linkEventDefinition id="_id_1"  name="nextPage">
     	<source>_id_2</source>
     </linkEventDefinition>
     
     <!-- catch link event -->
     <linkEventDefinition id="_id_2" name="nextPage">
     	<target>_id_1</target>
     </linkEventDefinition>   
     
     
     <sequenceFlow id='_481A2782-FC17-41CB-BC28-5799B6866A81' sourceRef='_id_2' targetRef='_6381BCBA-B49A-4BC1-AC3D-AEA745D9738E'/>
  
  
  
  <scriptTask id="_6381BCBA-B49A-4BC1-AC3D-AEA745D9738E" name="task after link">
      <script>System.out.println("jumped");</script>
    </scriptTask>
    
    
        <sequenceFlow id='_325E4E04-60D2-4E0A-89FC-CBFA2304D401' sourceRef='_6381BCBA-B49A-4BC1-AC3D-AEA745D9738E' targetRef='_8E0F0FAD-6087-4FA2-80BE-6976C87A4CE1'/>
    
    
    
    <endEvent id='_8E0F0FAD-6087-4FA2-80BE-6976C87A4CE1' name='stop doing'/>
    
  </process>
